{
  "compileOnSave": false,
  "compilerOptions": {
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [ "es2018", "dom" ],
    "mapRoot": "./",
    "module": "esnext",
    "skipLibCheck": true,
    "moduleResolution": "node",
    "outDir": "../dist/out-tsc",
    "sourceMap": true,
    "target": "es2015",
    "typeRoots": [
      "../node_modules/@types"
    ],
    "baseUrl": ".",
    "paths": {
      "@appshared/*": [
        "app/shared/*"
      ],
      "@core/*": [
        "app/core/*"
      ],
      "@testing/*": [
        "testing/*"
      ],
      "@env/*": [
        "environments/*"
      ],
      "@shared/*": [
        "shared/*"
      ],
      "@app/*": [
        "./app/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}